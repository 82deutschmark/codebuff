{
  "name": "evals",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": ".",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --build",
        "cwd": "evals"
      },
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": [
        "^build"
      ]
    },
    "run-eval-set": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run dist/run-eval-set.js",
        "cwd": "evals"
      },
      "dependsOn": [
        "build"
      ]
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "evals"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["@codebuff/integrations"]
}