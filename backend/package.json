{
  "name": "backend",
  "version": "1.0.40",
  "description": "Backend server for Manicode",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start-js": "node dist/index.js",
    "prebuild": "yarn clean && cd ../common && yarn build && node ../scripts/build-version-info.js",
    "build": "tsc --build && tsc-alias",
    "postbuild": "mkdir -p dist/common && cp -R ../common/dist/* dist/common && tsc-alias",
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "diff": "^5.1.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "*",
    "openai": "^4.55.4",
    "tiktoken": "^1.0.15",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/diff": "^5.0.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.2",
    "@types/ws": "^8.5.5",
    "jest": "^29.5.0",
    "nodemon": "^2.0.15",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@types/lodash": "*",
    "@types/node": "*"
  }
}
