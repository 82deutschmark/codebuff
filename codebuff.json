{
  "startupProcesses": [
    {
      "name": "drizzle",
      "command": "bun start-studio",
      "stdoutFile": "debug/drizzle.log",
      "stderrFile": "debug/drizzle.log"
    },
    {
      "name": "web server",
      "command": "bun start-web",
      "stdoutFile": "debug/web.log",
      "stderrFile": "debug/web.log"
    },
    {
      "name": "typechecker",
      "command": "bun typecheck --watch # this doesn't actually work for some reason",
      "enabled": false
    }
  ]
}
