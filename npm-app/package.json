{
  "name": "manicode",
  "version": "1.0.29",
  "description": "AI dev assistant",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "manicode": "dist/index.js"
  },
  "scripts": {
    "prebuild": "yarn clean && cd ../common && yarn build",
    "build": "tsc && tsc-alias",
    "postbuild": "mkdir -p dist/common && cp -r ../common/dist/* dist/common && tsc-alias",
    "clean": "rm -rf dist && (rm tsconfig.tsbuildinfo || true)",
    "prepublishOnly": "yarn build && node clean-package.js",
    "postpublish": "node -e \"const fs=require('fs');fs.copyFileSync('temp.package.json','package.json');fs.unlinkSync('temp.package.json');\"",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "start-js": "node dist/index.js ..",
    "start": "ts-node src/index.ts ..",
    "start-prod": "NODE_ENV=production ts-node src/index.ts",
    "ssh": "gcloud compute ssh manicode-backend --zone=us-east4-a"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10",
    "ignore": "^5.3.1",
    "lodash": "*",
    "ws": "^8.2.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/ws": "^8.2.0"
  },
  "peerDependencies": {
    "@types/lodash": "*",
    "@types/node": "*",
    "typescript": "*"
  }
}
